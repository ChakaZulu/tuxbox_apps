<html>
<head>
<title>enigma remote control</title>
<style type="text/css">
 a.volgreen { color:#00ff00; font-size:12pt; font-family: tahoma, arial; }
 a.volnot { color:#c0c0c0; font-size:12pt; font-family: tahoma, arial; }
 a.mute { color:#ffffff; font-size:8pt; font-family:tahoma, arial; }
 a.audio { color:#ffffff; font-size:10pt; font-family:tahoma, arial; }

 .white { color:#ffffff; font-size:9pt; font-family: tahoma, arial; }
 .titel { color:#ffffff; font-size:14pt; font-family: tahoma, arial; }
 .servicename { color:#ffffff; font-size:13pt; font-family: tahoma, arial; }
 .epgzeit { color:#ffffff; font-size:12pt; font-family: tahoma, arial; }
 .epgdur { color:#ffffff; font-size:10pt; font-family: tahoma, arial; }
 .epgname { color:#ffffff; font-size:12pt; font-family: tahoma, arial; }
 .epgdesc { color:#c0c0c0; font-size:10pt; font-family: tahoma, arial; }
 
 a.normal  { color:#ffffff; font-size:10pt; font-family: tahoma, arial; }
 a.white   { color:#ffffff; font-size:14pt; font-family: tahoma, arial; }
 
 a:link    { text-decoration:none; }
 a:visited { text-decoration:none; }
 a:active  { text-decoration:none; }
</style>
<script language="javascript">
 function getNewPageTV(xy)
 {
  document.location.href="/?mode=tv&bouquetid="+xy;
 }

 function getNewPageRadio(xy)
 {
  document.location.href="/?mode=radio&bouquetid="+xy;
 }

 function reload()
 {
  document.location.reload();
 }
 
 function setVol(xy)
 {
  win=window.open("/setVolume?volume="+xy, "switchStatus","width=50,height=20,left=0,top=0");
  win.focus(); 
  parent.setTimeout("reload()", 100);
 }

 function Mute()
 {
  win=window.open("/setVolume?mute=1", "switchStatus","width=50,height=20,left=0,top=0");
  win.focus();
  parent.setTimeout("reload()", 100);
 }

 function unMute()
 {
  win=window.open("/setVolume?mute=0", "switchStatus","width=50,height=20,left=0,top=0");
  win.focus();
  parent.setTimeout("reload()", 100);
 }


 function switchtoChannel(xy)
 {
  win=window.open("/switchTo?service="+xy, "switchStatus","width=50,height=20,left=0,top=0");
  win.focus();
  parent.setTimeout("reload()", 3000);
 }
 
</script>
</head>
<body bgcolor="#000000" text="#c0c0c0" marginwidth="0" marginheight="0">

<table width="100%" border="0">
<tr>
<td align="center">
 <table width="750" cellpadding="0" cellspacing="0" border="0">
  <tr>
   <td width="750" bgcolor="#ffffff"><img src="trans.gif"></td>
  </tr>
 </table>
 <table width="750" cellpadding="0" cellspacing="0" border="0">
  <tr>
   <td width="1" bgcolor="#ffffff"><img src="trans.gif"></td>
   <td width="200" bgcolor="#000000"><img src="logo.gif"></td>
   <td width="1" bgcolor="#ffffff"><img src="trans.gif"></td>
   <td width="547" bgcolor="#000000">
   <table width="100%" cellspacing="0" cellpadding="0" border="0">
    <tr>
     <td valign="top" align="center">#STATS#</td>
    </tr>
    <tr>
     <td valign="bottom" align="center">#NAVI#<br>#VOLBAR#</td>
    </tr>
   </table>
   </td>
   <td width="1" bgcolor="#ffffff"><img src="trans.gif"></td>
  </tr>
 </table>
 <table width="750" cellpadding="0" cellspacing="0" border="0">
  <tr>
   <td width="750" bgcolor="#ffffff"><img src="trans.gif"></td>
  </tr>
 </table>
 <table width="750" cellpadding="0" cellspacing="0" border="0">
  <tr>
   <td width="1" bgcolor="#ffffff"><img src="trans.gif"></td>
   <td width="10" bgcolor="#000000"><img src="trans.gif"></td>
   <td width="728" bgcolor="#000000">
    <table width="100%" cellspacing="0" cellpadding="0" border="0">
     <tr>
     <td width="100%" align="right">#MODE#</td>
     </tr>
     <tr>
     <td width="100%" align="left">#COP#<br><span class="servicename">#SERVICENAME#</span><br>#EIT#</td>
     </tr>
    </table>
   </td>
   <td width="10" bgcolor="#000000"><img src="trans.gif"></td>
   <td width="1" bgcolor="#ffffff"><img src="trans.gif"></td>
  </tr>
 </table>
 <table width="750" cellpadding="0" cellspacing="0" border="0">
  <tr>
   <td width="750" bgcolor="#ffffff"><img src="trans.gif"></td>
  </tr>
 </table>
</td>
</tr>
</table>
</body>
</html>
