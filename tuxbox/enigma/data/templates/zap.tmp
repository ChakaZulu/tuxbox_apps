<script language="javascript" type="text/javascript" src="zap.js"></script>
<script language="javascript" type="text/javascript">#ZAPDATA#</script>

<form name="channelselector">
	<table border=0 cellpadding=0 cellspacing=0>
		<tr>
			<td>
				<select style="width: 150px; font-weight: bold;" onChange=bouquetChange() name="bouquet" size=30></select>
			</td>
			<td>
				<select style="width: 470px; font-weight: bold;" ondblClick=channelChange() onkeyup=zapKeyUP() name="channel" size=30></select>
			</td>
		</tr>
	</table>
</form>
<table border=0 cellpadding=0 cellspacing=0>
	<tr>
		<td style="width: 210px">
			<input name="mepg" type="button" style="width: 100px; height: 22px; background-color: #CB0303" value="MEPG" onClick=javascript:mepg()>
			<input name="epg" type="button" style="width: 100px; height: 22px; background-color: #12259E" value="EPG" onClick=javascript:epg()>
		</td>
		<td style="width: 410px" align=center>
			<a href="javascript:zapChannelBackward()"><img src="up.gif" border="0"></a>
			&nbsp;&nbsp;
			<a href="javascript:zapChannelForward()"><img src="down.gif" border="0"></a>
		</td>
	</tr>
</table>
