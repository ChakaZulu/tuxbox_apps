<html>
	<head>
		<link rel="stylesheet" type="text/css" href="webif.css">
		<script language="javascript" type="text/javascript" src="window.js"></script>
		<script language="javascript" type="text/javascript" src="index.js"></script>
		<script language="javascript" type="text/javascript" src="timer.js"></script>
		<script language="javascript" type="text/javascript" src="zap.js"></script>
		<script language="javascript" type="text/javascript">#ZAPDATA#</script>
	</head>
	<body onLoad="init()" style="padding: 10px; background-color: #F4F4F4F4" leftmargin=0 topmargin=0 marginwidth=0 marginheight=0>
		<form name="channelselector">
			<table border=0 cellpadding=0 cellspacing=0>
				<tr>
					<td>
						<select style="width: 200px; font-weight: bold; background-color: #F4F4F4F4" onChange=bouquetChange() name="bouquet" size=#SELSIZE#></select>
					</td>
					<td>
						<select style="width: 430px; font-weight: bold; background-color: #F4F4F4F4" onChange=channelChange() name="channel" size=#SELSIZE#></select>
					</td>
				</tr>
			</table>
		</form>
		<table border=0 cellpadding=0 cellspacing=0>
			<tr>
				<td style="width: 200px">
					<input name="mepg" type="button" style="width: 100px; height: 22px; background-color: #CB0303; color: #FFFFFF;" value="EPG-Overview" onClick=javascript:mepg()>
			<!--input name="epg" type="button" style="width: 100px; height: 22px; background-color: #12259E" value="EPG" onClick=javascript:epg()-->
				</td>
				<td style="width: 430px" align=center>
					<a href="javascript:zapChannelBackward()"><img src="up.gif" border="0"></a>
					&nbsp;&nbsp;
					<a href="javascript:zapChannelForward()"><img src="down.gif" border="0"></a>
				</td>
			</tr>
		</table>
	</body>
</html>
