<script language="javascript" type="text/javascript" src="zap.js"></script>
<script language="javascript" type="text/javascript">#ZAPDATA#</script>

<form name="channelselector">
	<table border=0 cellpadding=0 cellspacing=0>
		<tr>
			<td style="width: 200px" align=center>
				<button name="mepg" style="width: 80px;" value="MEPG" onClick=mepg()>Multi-EPG</button>
				&nbsp;&nbsp;
				<button name="epg" style="width: 80px;" value="EPG" onClick=epg()>EPG</button>
			</td>
			<td style="width: 450px" align=center>
				<a href="javascript:zapChannelBackward()"><img src="player_rew.png" border="0"></a>
				&nbsp;&nbsp;
				<a href="javascript:zapChannelForward()"><img src="player_fwd.png" border="0"></a>
			</td>
		</tr>
	</table>
	<table border=0 cellpadding=0 cellspacing=0>
		<tr>
			<td>
				<select style="width: 200px; font-weight: bold;" onChange=bouquetChange() name="bouquet" size=30></select>
			</td>
			<td>
				<select style="width: 450px; font-weight: bold;" onChange=channelChange() name="channel" size=30></select>
			</td>
		</tr>
	</table>
</form>
