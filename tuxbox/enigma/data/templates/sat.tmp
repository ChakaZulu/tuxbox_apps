<script language="javascript" type="text/javascript" src="zap.js"></script>
<script language="javascript" type="text/javascript">#ZAPDATA#</script>
<form name="channelselector">
	<table border=0 cellpadding=0 cellspacing=0>
		<tr>
			<td>
				<select style="width: 150px; font-weight: bold;" onChange="bouquetChange()" name="bouquet" size=#SELSIZE#></select>
			</td>
			<td>
				<select style="width: 470px; font-weight: bold;" onChange="tuneTransponder('none')" name="channel" size=#SELSIZE#></select>
			</td>
		</tr>
	</table>
</form>
<table border=0 cellpadding=0 cellspacing=0>
	<tr>
		<td style="width: 210px">
			<input name="tune" type="button" style="width: 100px; height: 22px; background-color: #CB0303; color: #FFFFFF" value="Tune" onClick=javascript:tuneTransponder('none')>
		</td>
		<td style="width: 410px" align=center>
			<!--a href="javascript:zapChannelBackward()"><img src="up.gif" border="0"></a-->
			&nbsp;&nbsp;
			<!--a href="javascript:zapChannelForward()"><img src="down.gif" border="0"></a-->
		</td>
	</tr>
</table>
