<html>
	<head>
		<link rel="stylesheet" type="text/css" href="webif.css">
		<script language="javascript" type="text/javascript" src="window.js"></script>
		<script language="javascript" type="text/javascript" src="index.js"></script>
	</head>
	<body id="headbody">
		<table width=100% height=112 border=0 cellpadding=0 cellspacing=0 background=#TOPBALK#>
		<tr>
			<td width=150>&nbsp;</td>
			<td>
				<table id="headercell" width=100% border=0 cellpadding=0 cellspacing=0>
				<tr>
					<td>
						<table width=100% border=0 cellpadding=0 cellspacing=0>
						<tr>
							<td font style="font-family: Verdana; font-size: 12pt; font-weight: bold">#SERVICENAME#</font></td>
							<td align=right>#EMPTYCELL#</td>
						</tr>
						</table>
						<table width=100% height=100% border=0 cellpadding=0 cellspacing=0>
						<tr>
							<td width=40 align=left><span class="epgzeit"><b>#NOWT#</b></span></td>
							<td width=5>&nbsp;</td>
							<td width=50 align=left><span class="epgdur">#NOWD#</span></td>
							<td width=5>&nbsp;</td>
							<td width=320 Align=left><span class="epgname"><b>#NOWST#</b></span></td>
							<td width=5>&nbsp;</td>
							<td width=180 align=right>#VOLBAR#</td>
						</tr>
						<tr>
							<td width=40 align=left><span class="epgzeit">#NEXTT#</span></td>
							<td width=5>&nbsp;</td>
							<td width=50 align=left><span class="epgdur">#NEXTD#</span></td>
							<td width=5>&nbsp;</td>
							<td width=320 align=left><span class="epgname">#NEXTST#</b></td>
							<td width=5>&nbsp;</td>
							<td width=180 align=right>#RECORDING#&nbsp;#CHANSTATS#</td>
						</tr>
						</table>
						<table id="statcell" width=100% border=0 cellpadding=0 cellspacing=0 height=40>
						<tr>
							<td align=left><span class="white">#STATS#</span></td>
						</tr>
						</table>
					</td>
				</tr>
				</table>
			</td>
		</tr>
		</table>
		<table width=100% height=24 border=0 cellspacing=0 cellpadding=0>
		<tr>
			<td id="channavi">#CHANNAVI#</td>
		</tr>
		</table>
	</body>
</html>
