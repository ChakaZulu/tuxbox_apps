<html>
	<head>
		<link rel="stylesheet" type="text/css" href="webif.css">
		<script language="javascript" type="text/javascript" src="window.js"></script>
		<script language="javascript" type="text/javascript" src="index.js"></script>
		<script language="javascript" type="text/javascript" src="dhtml.js"></script>
		<script>
			function update()
			{
				if (parent.data.requestUpdateNow)
					parent.data.requestUpdateNow();
				else
					setTimeout("update()", 250);
			}
		</script>
	</head>
	<body id="headbody" background="#TOPBALK#" onLoad="update()">
		<table id="headTable" width="100%" height="112px" border="0" cellpadding="0" cellspacing="0">
		<tr>
			<td>
				<img width="150" height="0" border="0" src="trans.gif">
			</td>
			<td>
				<table id="headercell" width=100% border="0" cellpadding="0" cellspacing="0">
				<tr>
					<td>
						<table width=100% border="0" height="68px"cellpadding="0" cellspacing="0">
						<tr>
							<td id="servicename" colspan="6" class="servicename" colspan=6 align=left>&nbsp;</td>
							<td width="190px" id="empty" align=right>#EMPTYCELL#</td>
						</tr>
						<tr>
							<td id="nowt" class="epgzeit_bold" width=40 align=left>&nbsp;</td>
							<td width=5>&nbsp;</td>
							<td id="nowd" class="epgdur" width=50 align=left>&nbsp;</td>
							<td width=5>&nbsp;</td>
							<td id="nowst" class="epgname_bold" width=320 align=left>&nbsp;</td>
							<td width=5>&nbsp;</td>
							<td width=190px align=right>
								<table border="0" cellspacing="0" cellpadding="0">
								<tr>
									<td><a href="javascript:setVol(9)"><img id="imgVol9" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td><a href="javascript:setVol(15)"><img id="imgVol15" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td><a href="javascript:setVol(21)"><img id="imgVol21" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td><a href="javascript:setVol(27)"><img id="imgVol27" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td><a href="javascript:setVol(33)"><img id="imgVol33" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td><a href="javascript:setVol(39)"><img id="imgVol39" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td><a href="javascript:setVol(45)"><img id="imgVol45" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td><a href="javascript:setVol(51)"><img id="imgVol51" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td><a href="javascript:setVol(57)"><img id="imgVol57" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td><a href="javascript:setVol(63)"><img id="imgVol63" src="led_on.gif" border="0" width="15" height="8"></a></td>
									<td>&nbsp;</td> 
									<td><a href="javascript:toggleMute()"><img id="mute" border="0" src="speak_on.gif"></a></td>
								</tr>
								</table>
							</td>
						</tr>
						<tr>
							<td id="nextt" class="epgzeit" width=40 align=left>&nbsp;</td>
							<td width=5>&nbsp;</td>
							<td id="nextd" class="epgdur" width=50 align=left>&nbsp;</td>
							<td width=5>&nbsp;</td>
							<td id="nextst" class="epgname" width=320 align=left>&nbsp;</td>
							<td width=5>&nbsp;</td>
							<td width="190px" align="right">
								<img id="recording" width="12" border="0" src="trans.gif">
								<img height="0" width="5" border="0" src="trans.gif">
								<img id="imgDolby" src="dolby_off.png" border="0">
								<img height="0" width="5" border="0" src="trans.gif">
								<img id="imgCrypt" src="crypt_off.png" border="0">
								<img height="0" width="5" border="0" src="trans.gif">
								<img id="imgFormat" src="format_off.png" border="0">
							</td>
						</tr>
						</table>
						<table id="statcell" border="0" cellpadding="0" cellspacing="0" height="34px">
						<tr>
							<td id="lock">&nbsp;</td>
							<td>&nbsp;<img src="squ.png">&nbsp;</td>
							<td>disk space:&nbsp;</td>
