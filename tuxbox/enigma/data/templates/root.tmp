<script language="javascript" type="text/javascript" src="zap.js"></script>
<script language="javascript" type="text/javascript">#ZAPDATA#</script>

<form name="channelselector">
	<table border=0 cellpadding=0 cellspacing=0>
		<tr>
			<td>
				<select style="width: 0px; font-weight: bold; background-color: #F4F4F4" onChange=bouquetChange() name="bouquet" size=0></select>
			</td>
			<td>
				<select style="width: 630px; font-weight: bold; background-color: #F4F4F4" onChange=channelChange() name="channel" size=#SELSIZE#></select>
			</td>
		</tr>
	</table>
</form>
<table border=0 cellpadding=0 cellspacing=0>
	<tr>
		<td style="width: 210px">
			#BUTTON#
		</td>
		<td style="width: 410px" align=center>
			<a href="javascript:zapChannelBackward()"><img src="up.gif" border="0"></a>
			&nbsp;&nbsp;
			<a href="javascript:zapChannelForward()"><img src="down.gif" border="0"></a>
		</td>
	</tr>
</table>
