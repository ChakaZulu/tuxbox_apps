<html>
	<head>
		<title>Enigma PDA Web Interface</title>
		<link rel="stylesheet" type="text/css" href="webif_small.css">
		<script language="javascript" type="text/javascript" src="window.js"></script>
		<script language="javascript" type="text/javascript" src="index.js"></script>
		<script language="javascript" type="text/javascript" src="timer.js"></script>
		<script language="javascript" type="text/javascript" scr="epg.js"></script>
	</head>
	<body leftmargin=0 topmargin=0 marginwidth=0 marginheight=0 id="mainbody">
		<div align="center">
			<table width=240 height=100% border=0 cellpadding=0 cellspacing=0 id="megatable">
				<tr>
					<td height=45 background=#TOPBALK# align=right valign=top>
						<table border=0 cellspacing=0 cellpadding=5>
						<tr>
							<td><img src="trans.gif" width=50 height=1 border=0></td>
							<td>
								<table border=0 cellspacing=0 cellpadding=0>
								<tr>
									#VOLBAR#
									<td>#MUTE#</td>
								</tr>
								</table>
							</td>
						</tr>
						</table>
					</td>
				</tr>
				<tr>
					<td height=14 id="channavi">#CHANNAVI#</td>
				</tr>
				<tr>
					<td height=14 id="topnavi">#TOPNAVI#</td>
				</tr>
				<tr>
					<td valign=top>
						<table width=100% height=100% border=0 cellspacing=0 cellpadding=0>
							<tr>
								<td align=right valign=top width=55 class=menu id="leftnavi">
									#LEFTNAVI#
								</td>
								<td align=left valign=top id="contentcell">
									#CONTENT#
								</td>
							</tr>
						</table>
					</td>
				</tr>
			</table>
		</div>
		<br>
	</body>
</html>
