<!DOCTYPE skin SYSTEM "skin.dtd" [
<!ENTITY lt "<">
<!ENTITY gt ">">
<!ENTITY auml "&#228;">
<!ENTITY ouml "&#246;">
<!ENTITY uuml "&#252;">
<!ENTITY Auml "&#196;">
<!ENTITY Ouml "&#214;">
<!ENTITY Uuml "&#220;">
<!ENTITY szlig "&#223;">
]>
<eskin>
	<author>
		<name>Steffen Hehn</name>
		<email>McClean@sth-tech.de</email>
	</author>
	
	<colors>
		<color name="background" color="#ff000000" />

		<color name="header" color="#030FA4" end="#FFE403" size="16" />
		<color name="content" color="#33030F60" end="#ffffff" size="16" />
		<color name="content_selected" color="#90A0FF" end="#000F33" size="16" />

		<color name="osd_normal" color="#000F9A" end="#ffffff" size="16" />
		<color name="osd_epg" color="#000F9A" end="#a0a0f0" size="16" />
		<color name="osd_shadowcol" color="#030320" />
		<color name="osd_shadow" color="#030320" end="#B1B2D9" size="16" />
		<color name="osd_timebar" color="#1010AA"/>
		<color name="osd_timebarborder" color="#ffffff"/>

		<color name="shadow_red" color="#030320" end="#ff0303" size="16" />
		<color name="shadow_green" color="#030320" end="#03ff03" size="16" />
		<color name="shadow_yellow" color="#030320" end="#ffff03" size="16" />
		<color name="shadow_blue" color="#030320" end="#0303ff" size="16" />
		<color name="shadow_gray" color="#030320" end="#282828" size="16" />
	</colors>
	
	<colorscheme>
		<map name="backgroundColor" color="content" />

		<map name="eServiceSelector.highlight" color="content_selected" />
		<map name="focusedColor" color="content_selected" />

		<map name="fgColor" color="content" />
		<map name="fgColorDisabled" color="content" />

		<map name="eWindow.titleBarColor" color="header" />

		<map name="eProgress.border" color="osd_normal+15" />
		<map name="eProgress.left" color="osd_normal+10" />
		<map name="eProgress.right" color="osd_normal+5" />
		
	</colorscheme>
	
	 <values>
		<value name="eWindow.titleFontSize" value="25" />
		<value name="eWindow.borderSize" value="0" />
		<value name="eWindow.titleOffsetX" value="10" />
		<value name="eWindow.titleOffsetY" value="0" />
		<value name="eWindow.titleSize" value="32" />
	</values>

	<images basepath="" target="fb">
		<img name="osd_crypt_on" src="crypt_on.png" />
		<img name="osd_format_on" src="format_on.png" />
		<img name="osd_dolby_on" src="dolby_on.png" />
		<img name="osd_dolby_off" src="dolby_off.png" />
		<img name="osd_crypt_off" src="crypt_off.png" />
		<img name="osd_format_off" src="format_off.png" />


		<!--
		<img name="eWindow.top" src="header.png" />
		<img name="eWindow.topLeft" src="header.png" />
		<img name="eWindow.topRight" src="header.png" />
		<img name="eWindow.left" src="header.png" />
		<img name="eWindow.right" src="header.png" />
		<img name="eWindow.bottomLeft" src="bottom.png" />
		<img name="eWindow.bottom" src="bottom.png" />
		<img name="eWindow.bottomRight" src="bottom.png" />
		-->
	</images>

	<object name="test">
		<eWindow position="75:75" size="500:300" text="Skinned testwindow">
			<eLabel position="0:0" size="480:50" text="Hello world."
				font="Courier New Regular;26"/>
			<eLabel position="50:50" size="430:100" text="you suck\nblabla"/>
			<eButton position="380:200" size="100:50" text="OK" name="okButton" />
			<eButton position="280:200" size="100:50" text="Cancel" name="cancelButton" />
		</eWindow>
	</object>

	<object name="eventview">
		<eWindow position="50:70" size="600:420" name="title">
			<eLabel position="10:5" size="590:355" name="epg_description" font="NimbusSansL-Regular Sans L Regular;24" backgroundColor="content" align="wrap"/>
			<eWidget position="0:367" size="600:40" backgroundColor="osd_shadowcol" />
			<eLabel position="10:367" size="130:40" name="date" font="NimbusSansL-Regular Sans L Regular;18" backgroundColor="osd_shadow"/>
			<eLabel position="130:367" size="230:40" name="time" font="NimbusSansL-Regular Sans L Regular;18" backgroundColor="osd_shadow"/>
			<eLabel position="360:367" size="230:40" text="sat1" font="NimbusSansL-Regular Sans L Regular;18" backgroundColor="osd_shadow" align="right"/>
		</eWindow>
	</object>

	<object name="enigma_lcd">
		<eZapLCDMain position="0:0" size="120:64">
			<eLabel position="0:0" size="70:18" font="NimbusSansL-Regular Sans L Regular;16" text="Enigma" />
			<eLabel position="0:20" size="120:30" font="NimbusSansL-Regular Sans L Regular;15" wrap="on" name="service_name" />
			<eLabel position="74:48" size="46:16" font="NimbusSansL-Regular Sans L Regular;16" name="clock" />
			<eProgress position="0:55" size="70:6" name="volume_bar" foregroundColor="255" leftColor="255" rightColor="0" border="1" />
		</eZapLCDMain>
	</object> 
	
	<object name="ezap_main">
		<eZapMain position="35:390" size="710:160">

			<!--kanalnummer-->
			<eWidget position="4:3" size="113:62" backgroundColor="osd_normal" />

			<!--hauptbox-->
			<eWidget position="127:40" size="488:35" backgroundColor="osd_normal" />
			<eWidget position="34:75" size="581:75" backgroundColor="osd_normal" />

			<!--schatten-->
			<eWidget position="14:65" size="113:10" backgroundColor="osd_shadowcol" />
			<eWidget position="117:15" size="10:60" backgroundColor="osd_shadowcol" />
			<eWidget position="127:15" size="488:25" backgroundColor="osd_shadowcol" />
			<eWidget position="44:150" size="579:10" backgroundColor="osd_shadowcol" />
			<eWidget position="613:15" size="10:135" backgroundColor="osd_shadowcol" />

			<!--
			<eWidget position="560:49" size="95:20" backgroundColor="osd_timebarborder" />
			<eWidget position="562:51" size="91:16" backgroundColor="osd_timebar" />
			-->

			<!--texte-->
			<eLabel position="10:5" size="100:50" font="NimbusSansL-Regular Sans L Regular;50" name="ch_number" align="center"
				backgroundColor="osd_normal"/>

			<eLabel position="140:41" size="400:35" font="NimbusSansL-Regular Sans L Regular;35" name="ch_name"
				backgroundColor="osd_normal"/>

			<eLabel position="50:82" size="75:27" font="NimbusSansL-Regular Sans L Regular;25" name="e_now_time"
				backgroundColor="osd_epg"/>
			<eLabel position="50:114" size="75:27" font="NimbusSansL-Regular Sans L Regular;25" name="e_next_time"
				backgroundColor="osd_epg"/>

			<eLabel position="140:82" size="365:35" font="NimbusSansL-Regular Sans L Regular;25" name="e_now_title"
				backgroundColor="osd_epg"/>
			<eLabel position="140:114" size="365:35" font="NimbusSansL-Regular Sans L Regular;25" name="e_next_title"
				backgroundColor="osd_epg"/>

			<eLabel position="505:82" size="95:35" font="NimbusSansL-Regular Sans L Regular;25" name="e_now_duration" align="right"
				backgroundColor="osd_epg"/>
			<eLabel position="505:114" size="95:35" font="NimbusSansL-Regular Sans L Regular;25" name="e_next_duration" align="right"
				backgroundColor="osd_epg"/>

			<eLabel position="536:15" size="65:21" font="NimbusSansL-Regular Sans L Regular;21" name="time" align="right"
				backgroundColor="osd_shadow"/>

			<eProgress position="505:49" size="95:20" name="progress_bar" />

			<ePixmap position="127:17" size="104:24" pixmap="osd_crypt_on" name="osd_crypt_on" />
			<ePixmap position="150:19" size="104:24" pixmap="osd_format_on" name="osd_format_on"/>
			<ePixmap position="195:21" size="104:24" pixmap="osd_dolby_on" name="osd_dolby_on"/>
			<ePixmap position="150:19" size="104:24" pixmap="osd_format_off" name="osd_format_off"/>
			<ePixmap position="195:21" size="104:24" pixmap="osd_dolby_off" name="osd_dolby_off"/>
			<ePixmap position="127:17" size="104:24" pixmap="osd_crypt_off" name="osd_crypt_off"/>
			<eLabel name="description" />
			
			<eLabel name="button_red_enabled" position="230:19" size="20:20" font="Marlett Regular;20" backgroundColor="shadow_red" />
			<eLabel name="button_green_enabled" position="250:19" size="20:20" font="Marlett Regular;20" backgroundColor="shadow_green"  />
			<eLabel name="button_yellow_enabled" position="270:19" size="20:20" font="Marlett Regular;20" backgroundColor="shadow_yellow"  />
			<eLabel name="button_blue_enabled" position="290:19" size="20:20" font="Marlett Regular;20" backgroundColor="shadow_blue" />

			<eLabel name="button_red_disabled" position="230:19" size="20:20" font="Marlett Regular;20" backgroundColor="shadow_gray"  />
			<eLabel name="button_green_disabled" position="250:19" size="20:20" font="Marlett Regular;20" backgroundColor="shadow_gray"  />
			<eLabel name="button_yellow_disabled" position="270:19" size="20:20" font="Marlett Regular;20" backgroundColor="shadow_gray"  />
			<eLabel name="button_blue_disabled" position="290:19" size="20:20" font="Marlett Regular;20" backgroundColor="shadow_gray"  />
	
		</eZapMain>
	</object>
</eskin>
