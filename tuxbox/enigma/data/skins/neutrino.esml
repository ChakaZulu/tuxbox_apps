<eskin>
	<author>
		<name>Steffen Hehn</name>
		<email>McClean@sth-tech.de</email>
	</author>

	<colors>
		<color name="header" color="#030FA4" end="#FFE403" size="16" />
		<color name="content" color="#22030F60" end="#ffffff" size="16" />
		<color name="content_selected" color="#2290A0FF" end="#000F33" size="16" />

		<color name="osd_normal" color="#11000F9A" end="#ffffff" size="16" />
		<color name="osd_epg" color="#11000F9A" end="#9090e0" size="16" />
		<color name="osd_shadowcol" color="#11030320" />
		<color name="osd_shadow" color="#11030320" end="#B1B2D9" size="16" />
		<color name="osd_timebar" color="#1010AA"/>
		<color name="osd_timebarborder" color="#ffffff"/>

		<color name="shadow_red" color="#11030320" end="#ff0303" size="16" />
		<color name="shadow_green" color="#11030320" end="#03ff03" size="16" />
		<color name="shadow_yellow" color="#11030320" end="#ffff03" size="16" />
		<color name="shadow_blue" color="#11030320" end="#0303ff" size="16" />
		<color name="shadow_gray" color="#11030320" end="#282828" size="16" />
	</colors>

	<colorscheme>
		<map name="global.normal.background" color="content" />
		<map name="global.normal.foreground" color="content+15" />

		<map name="global.selected.background" color="content_selected" />
		<map name="global.selected.foreground" color="content_selected+15" />

		<map name="global.disabled.background" color="content" />
		<map name="global.disabled.foreground" color="content+7" />

		<map name="eWindow.titleBar" color="header" />
		<map name="eWindow.titleBarFont" color="header+15" />

		<map name="eServiceSelector.highlight.background" color="content_selected" />
		<map name="eServiceSelector.highlight.foreground" color="content_selected+15" />

		<map name="eProgress.border" color="osd_normal+15" />
		<map name="eProgress.left" color="osd_normal+10" />
		<map name="eProgress.right" color="osd_normal+5" />
	</colorscheme>

	<values>
		<value name="eWindow.titleFontSize" value="25" />
		<value name="eWindow.borderLeft" value="0" />
		<value name="eWindow.borderRight" value="0" />
		<value name="eWindow.borderBottom" value="0" />
		<value name="eWindow.borderTop" value="32" />
		<value name="eWindow.titleOffsetX" value="10" />
		<value name="eWindow.titleOffsetY" value="0" />
		<value name="eWindow.titleHeight" value="32" />
		<value name="fontsize" value="24" />
	</values>

	<images basepath="" target="fb">
		<img name="osd_crypt_on" src="crypt_on.png" />
		<img name="osd_format_on" src="format_on.png" />
		<img name="osd_dolby_on" src="dolby_on.png" />
		<img name="osd_dolby_off" src="dolby_off.png" />
		<img name="osd_crypt_off" src="crypt_off.png" />
		<img name="osd_format_off" src="format_off.png" />
		<img name="enigma_logo" src="enigma2.png" nomerge="yes" />
		<img name="lcd_menu" src="lcdmenu.png" nomerge="yes" />
		<img name="lcd_scart" src="video.png" nomerge="yes" />
		<img name="lb_arrow" src="lb_arrow.png" />
	</images>

	<object name="eventview">
		<eWindow cposition="50:80" csize="590:400" name="title">
			<eLabel position="10:5" size="e:e-30" name="epg_description" font="NimbusSansL-Regular Sans L Regular;24" backgroundColor="content" foregroundColor="content+15" wrap="on"/>
			<eWidget position="0:e-30" size="600:e" backgroundColor="osd_shadowcol" />
			<eLabel position="10:e-30" size="130:e" name="date" font="NimbusSansL-Regular Sans L Regular;18" backgroundColor="osd_shadow" foregroundColor="osd_shadow+15" />
			<eLabel position="130:e-30" size="230:e" name="time" font="NimbusSansL-Regular Sans L Regular;18" backgroundColor="osd_shadow" foregroundColor="osd_shadow+15" />
			<eLabel position="360:e-30" size="230:e" name="channel" font="NimbusSansL-Regular Sans L Regular;18" backgroundColor="osd_shadow" foregroundColor="osd_shadow+15" align="right"/>
		</eWindow>
	</object>

	<object name="eServiceSelector">
		<eServiceSelector cposition="70:70" csize="580:460" text="Services">
			<eListbox name="services" position="10:5" size="e:e" font="NimbusSansL-Regular Sans L Regular;24" pixmap="lb_arrow"/>
		</eServiceSelector>
	</object>

	<object name="ezap_main">
		<eZapMain position="50:355" size="670:180">

			<!--kanalnummer-->
			<eWidget position="4:3" size="113:82" backgroundColor="osd_normal" />

			<!--hauptbox-->
			<eWidget position="127:60" size="468:35" backgroundColor="osd_normal" />
			<eWidget position="34:95" size="561:75" backgroundColor="osd_normal" />

			<!--schatten-->
			<eWidget position="14:85" size="113:10" backgroundColor="osd_shadowcol" />
			<eWidget position="117:15" size="10:80" backgroundColor="osd_shadowcol" />
			<eWidget position="127:35" size="478:25" backgroundColor="osd_shadowcol" />
			<eWidget position="44:170" size="561:10" backgroundColor="osd_shadowcol" />
			<eWidget position="595:35" size="10:135" backgroundColor="osd_shadowcol" />

			<!--
			<eWidget position="560:69" size="95:20" backgroundColor="osd_timebarborder" />
			<eWidget position="562:71" size="91:16" backgroundColor="osd_timebar" />
			-->

			<!--texte-->
			<eLabel position="10:5" size="100:50" font="NimbusSansL-Regular Sans L Regular;50" name="ch_number" align="center"
				backgroundColor="osd_normal" foregroundColor="osd_normal+15" />

			<eLabel position="140:61" size="445:40" font="NimbusSansL-Regular Sans L Regular;34" name="ch_name"
				backgroundColor="osd_normal" foregroundColor="osd_normal+15" />

			<eLabel position="50:102" size="75:27" font="NimbusSansL-Regular Sans L Regular;25" name="e_now_time"
				backgroundColor="osd_epg" foregroundColor="osd_epg+15" />
			<eLabel position="50:134" size="75:27" font="NimbusSansL-Regular Sans L Regular;25" name="e_next_time"
				backgroundColor="osd_epg" foregroundColor="osd_epg+15" />

			<eLabel position="140:102" size="345:35" font="NimbusSansL-Regular Sans L Regular;25" name="e_now_title"
				backgroundColor="osd_epg" foregroundColor="osd_epg+15" />
			<eLabel position="140:134" size="345:35" font="NimbusSansL-Regular Sans L Regular;25" name="e_next_title"
				backgroundColor="osd_epg" foregroundColor="osd_epg+15" />

			<eLabel position="495:102" size="90:35" font="NimbusSansL-Regular Sans L Regular;25" name="e_now_duration" align="right"
				backgroundColor="osd_epg" foregroundColor="osd_epg+15" />
			<eLabel position="495:134" size="90:35" font="NimbusSansL-Regular Sans L Regular;25" name="e_next_duration" align="right"
				backgroundColor="osd_epg" foregroundColor="osd_epg+15" />

			<eLabel position="521:35" size="65:21" font="NimbusSansL-Regular Sans L Regular;21" name="time" align="right"
				backgroundColor="osd_shadow" foregroundColor="osd_shadow+15" />

			<eProgress position="14:62" size="95:17" name="progress_bar" foregroundColor="osd_normal" border="1" />

			<eLabel position="140:37" size="104:24" pixmap="osd_crypt_on" name="osd_crypt_on" />
			<eLabel position="163:39" size="104:24" pixmap="osd_format_on" name="osd_format_on"/>
			<eLabel position="208:41" size="104:24" pixmap="osd_dolby_on" name="osd_dolby_on"/>
			<eLabel position="163:39" size="104:24" pixmap="osd_format_off" name="osd_format_off"/>
			<eLabel position="208:41" size="104:24" pixmap="osd_dolby_off" name="osd_dolby_off"/>
			<eLabel position="140:37" size="104:24" pixmap="osd_crypt_off" name="osd_crypt_off"/>
			<eLabel name="description" />

			<eLabel position="246:42" size="12:12" pixmap="button_red" name="button_red_enabled" alphatest="on" />
			<eLabel position="266:42" size="12:12" pixmap="button_green" name="button_green_enabled" alphatest="on" />
			<eLabel position="286:42" size="12:12" pixmap="button_yellow" name="button_yellow_enabled" alphatest="on" />
			<eLabel position="306:42" size="12:12" pixmap="button_blue" name="button_blue_enabled" alphatest="on" />

			<eLabel position="246:42" size="12:12" pixmap="button_gray" name="button_red_disabled" alphatest="on" />
			<eLabel position="266:42" size="12:12" pixmap="button_gray" name="button_green_disabled" alphatest="on" />
			<eLabel position="286:42" size="12:12" pixmap="button_gray" name="button_yellow_disabled" alphatest="on" />
			<eLabel position="306:42" size="12:12" pixmap="button_gray" name="button_blue_disabled" alphatest="on" />
	
		</eZapMain>
	</object>
</eskin>
